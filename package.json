{
  "name": "miningos-wrk-powermeter-satec",
  "version": "0.0.1",
  "description": "MiningOS Worker PowerMeter Satec",
  "keywords": [
    "tether"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/miningos-wrk-powermeter-satec.git"
  },
  "engine": {
    "node": ">=20.0"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "brittle tests/unit/*.test.js",
    "test:coverage": "brittle --coverage tests/{,**/}*.test.js",
    "mock": "DEBUG=* node mock/server.js --type PM180 -p 4015 --mockControlPort 5015 -h 0.0.0.0",
    "worker": "node worker.js --wtype wrk-powermeter-rack-PM180 --env development --rack rack-0"
  },
  "dependencies": {
    "@bitfinex/lib-js-util-promise": "git+https://github.com/bitfinexcom/lib-js-util-promise.git",
    "async": "3.2.6",
    "debug": "4.4.1",
    "miningos-lib-stats": "git+https://github.com/tetherto/miningos-lib-stats.git",
    "miningos-tpl-wrk-powermeter": "git+https://github.com/tetherto/miningos-tpl-wrk-powermeter.git",
    "svc-facs-modbus": "git+https://github.com/tetherto/svc-facs-modbus.git"
  },
  "devDependencies": {
    "miningos-mock-control-service": "git+https://github.com/tetherto/miningos-mock-control-service.git",
    "brittle": "3.16.3",
    "modbus-stream": "0.46.0",
    "standard": "17.1.2"
  },
  "overrides": {
    "serialport": "npm:noop@1.0.0"
  }
}
